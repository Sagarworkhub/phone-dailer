{
    "compilerOptions": {
        "jsx": "react",
        "baseUrl": ".",
        "paths": {
            "~/*": [
                "src/*"
            ],
            "~assets/*": [
                "src/assets/*"
            ],
            "~calling-screen/*": [
                "src/pages/calling-screen/*"
            ],
            "~components/*": [
                "src/components/*"
            ],
            "~dialer-screen/*": [
                "src/components/dialer-screen/*"
            ],
            "~home-screen/*": [
                "src/pages/home-screen/*"
            ],
            "~incoming-call-screen/*": [
                "src/pages/incoming-call-screen/*"
            ],
            "~keyboard/*": [
                "src/components/keyboard/*"
            ],
            "~message-screen/*": [
                "src/pages/message-screen/*"
            ],
            "~pages/*": [
                "src/pages/*"
            ],
            "~tag-input/*": [
                "src/components/tag-input/*"
            ],
            "~utils/*": [
                "src/utils/*"
            ]
        }
    },
    "files": [],
    "references": [
        {
            "path": "./tsconfig.app.json"
        },
        {
            "path": "./tsconfig.node.json"
        }
    ]
}